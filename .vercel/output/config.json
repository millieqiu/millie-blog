{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/archive\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/atom\\.xml\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/categories\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/categories(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/posts\\/page\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/posts(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}